<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aware.plugin.bluetooth_beacon_detect">

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">
        <service
            android:name="com.aware.plugin.bluetooth_beacon_detect.Plugin"
            android:exported="true" />

        <activity
            android:name="com.aware.plugin.bluetooth_beacon_detect.Settings"
            android:exported="true" />

        <provider
            android:name="com.aware.plugin.bluetooth_beacon_detect.Provider"
            android:authorities="${applicationId}.provider.all_beacons"
            android:exported="true"
            android:readPermission="com.aware.READ_CONTEXT_DATA"
            android:writePermission="com.aware.WRITE_CONTEXT_DATA" />

        <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    </application>

</manifest>